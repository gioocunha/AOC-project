/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=a,out=outa);
    Nand(a=b,b=b,out=outb);
    Nand(a=outa,b=b,out=s1);
    Nand(a=a,b=outb,out=s2);
    Nand(a=s1,b=s2,out=out);



}
